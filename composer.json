{
    "name": "php-etl/metadata",
    "description": "Describe data structures, to auto-configure and handle data transformation and data manipulation.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
